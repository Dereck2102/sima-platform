{
  "name": "auth-service",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps-services/auth-service",
        "main": "apps-services/auth-service/src/main.ts",
        "tsConfig": "apps-services/auth-service/tsconfig.app.json",
        "assets": [],
        "bundle": false,
        "platform": "node",
        "format": ["cjs"],
        "external": ["kafkajs"]
      }
    },
    "serve": {
      "executor": "@nx/node:execute",
      "options": {
        "buildTarget": "auth-service:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps-services/auth-service/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps-services/auth-service/src/**/*.ts"]
      }
    }
  },
  "tags": ["service", "api"]
}
