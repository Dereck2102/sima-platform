{
  "name": "analytics-engine",
  "projectType": "application",
  "sourceRoot": "apps/analytics-engine",
  "tags": ["scope:analytics", "type:python"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python -m uvicorn main:app --reload --port 3010",
        "cwd": "apps/analytics-engine"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pip install -r requirements.txt"
        ],
        "cwd": "apps/analytics-engine"
      }
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -t sima-analytics-engine .",
        "cwd": "apps/analytics-engine"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python -m pytest",
        "cwd": "apps/analytics-engine"
      }
    }
  }
}
